{
  "name": "harbour-lane-shipping-module",
  "version": "1.0.0",
  "description": "Shopify custom app for shipping logic based on warehouse postcode coverage",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "seed": "node src/db/seed.js",
